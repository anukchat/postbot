VITE_SUPABASE_URL=your_supabase_url_here
VITE_SUPABASE_ANON_KEY=your_supabase_anon_key_here

# Application URLs
VITE_REDIRECT_URL=http://localhost:5173/app
VITE_API_URL=http://localhost:8000
VITE_ENV=development

