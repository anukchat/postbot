{
  "version": 2,
  "builds": [
    {
      "src": "Dockerfile.backend",
      "use": "@vercel/docker"
    },
    {
      "src": "Dockerfile.frontend",
      "use": "@vercel/docker"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "Dockerfile.backend"
    },
    {
      "src": "/(.*)",
      "dest": "Dockerfile.frontend"
    }
  ]
}
